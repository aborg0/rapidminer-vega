  <!-- Your own operator factory -->
  <factory class = "my.new.operators.OperatorFactory" />

  <!-- Your own Operators -->
  <operator 
    name        = "MyExampleSetWriter" 
    class       = "my.new.operators.ExampleSetWriter"
